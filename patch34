+                if (Fisma.AutoComplete.resultsPopulated) {
+                    ac.expandContainer();
+                }
+            };
+            
+            /**
+             * Record the fact that the results have been retrieved
+             */
+            ac.containerPopulateEvent.subscribe(function () {
